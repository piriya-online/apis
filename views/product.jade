extends layout

block content
	div.page-header
		h2 #{title} 
			small : ข้อมูลสินค้า

	div#accordion.panel-group(role='tablist', aria-multiselectable='true')


		div.panel.panel-default
			div#headingOne.panel-heading(role='tab')
				h4.panel-title
					a(data-toggle='collapse', data-parent='#accordion', href='#collapseAllActiveProduct', aria-expanded='false')
						strong All Active Product
						&#58; ข้อมูลสินค้าทั้งหมดที่ยังขายอยู่
			div#collapseAllActiveProduct.panel-collapse.collapse(role='tabpanel')
				div.panel-body
					pre URL : /product/all
					div.panel.panel-default
						div.panel-body
							form.form-horizontal
								div.form-group
									label.col-sm-2.control-label(for='input') Authentication Key
									div.col-sm-10
										input.form-control.authKey(type='text', placeholder='Authentication Key')
										p.help-block 
											span.label.label-default authKey
											|   รหัส Authentication
								div.form-group.margin-bottom-0
									div.col-sm-offset-2.col-sm-10
										div.btn.btn-primary.btn-submit(data-loading-text='Loading...', autocomplete='off') Submit
					div.result.hidden.alert.alert-info.margin-top-10.margin-bottom-0.font-pre(role='alert')


		div.panel.panel-default
			div.panel-heading(role='tab')
				h4.panel-title
					a(data-toggle='collapse', data-parent='#accordion', href='#collapseProductCategory', aria-expanded='false')
						strong Category 
						&#58; ข้อมูลหมวดหมู่สินค้า

			div#collapseProductCategory.panel-collapse.collapse(role='tabpanel')
				div.panel-body
					pre URL : /product/category
					div.panel.panel-default
						div.panel-body
							form.form-horizontal
								div.form-group
									label.col-sm-2.control-label(for='input') Shop ID
									div.col-sm-10
										input.form-control.shop(type='text', placeholder='Shop ID')
										p.help-block 
											span.label.label-warning shop
											|   รหัสร้านค้า
								div.form-group.margin-bottom-0
									div.col-sm-offset-2.col-sm-10
										div.btn.btn-primary.btn-submit(data-loading-text='Loading...', autocomplete='off') Submit
					div.result.hidden.alert.alert-info.margin-top-10.margin-bottom-0.font-pre(role='alert')

		div.panel.panel-default
			div.panel-heading(role='tab')
				h4.panel-title
